INTEGRATION_REDIS_URL=redis://localhost:6379/1

# Optional: override default repos/queues when running locally
DATABASE_URL=postgresql://<user>:<password>@localhost:5432/<db>
POSTGRES_URL=postgresql://<user>:<password>@localhost:5432/<db>
REDIS_URL=redis://localhost:6379/1
PROFILE_KEY_ENC_KEY=change-me-32-bytes-minimum
PROFILE_KEY_STORE=postgres
AGENT_TOKEN_STORE=postgres
NARRATIVE_STORE=postgres
ORCHESTRATOR_URL=http://localhost:3002
AGENT_RATE_LIMIT_MAX=30
AGENT_RATE_LIMIT_WINDOW_MS=60000
