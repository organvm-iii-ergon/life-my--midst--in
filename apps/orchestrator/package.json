{
  "name": "@in-midst-my-life/orchestrator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "COVERAGE=true vitest run",
    "integration": "vitest run test/integration.test.ts",
    "migrate": "tsx src/scripts/migrate.ts",
    "seed": "tsx src/scripts/seed.ts",
    "test:integration": "vitest run test/integration.test.ts"
  },
  "dependencies": {
    "@in-midst-my-life/core": "workspace:*",
    "@in-midst-my-life/schema": "workspace:*",
    "exifr": "^7.1.3",
    "fastify": "^5.7.3",
    "ioredis": "^5.4.1",
    "jose": "^6.1.3",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.13.1",
    "sharp": "^0.34.5",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/unzipper": "^0.10.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.0.9"
  }
}
